                                android:text="12#"
                                android:textColor="#000"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/xDone"
                                android:layout_width="@dimen/margin_40dp"
                                android:layout_height="fill_parent"
                                android:layout_gravity="right"
                                android:background="@drawable/backspace_default"
                                android:fitsSystemWindows="true"
                                android:soundEffectsEnabled="true"
                                android:tag="done"
                                android:textColor="#000"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </TableRow>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/Adress_rl"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="@dimen/margin_20dp"
                        android:visibility="visible">

                        <ImageView
                            android:id="@+id/mBackBtn"
                            android:layout_width="@dimen/height_60dp"
                            android:layout_height="@dimen/height_60dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/margin_70dp"
                            android:src="@drawable/back_to_call"
                            android:visibility="gone" />

                        <ImageView
                            android:id="@+id/softSecureCalling"
                            android:layout_width="@dimen/margin_60dp"
                            android:layout_height="@dimen/margin_60dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:background="@drawable/ripple_effect"
                            android:src="@drawable/securecallicon"
                            android:visibility="visible" />

                        <ImageView
                            android:id="@+id/mSecureConference"
                            android:layout_width="@dimen/height_60dp"
                            android:layout_height="@dimen/height_60dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/margin_70dp"
                            android:src="@drawable/add_call"
                            android:visibility="gone" />

                    </RelativeLayout>


                </TableLayout>
            </TableLayout>
        </RelativeLayout>
    </RelativeLayout>


</RelativeLayout>